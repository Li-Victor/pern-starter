{
  "name": "pern",
  "version": "1.0.0",
  "description": "Postgres Express React/Redux/React-Router-DOM Node stack boilerplate",
  "main": "src/index.js",
  "author": "Victor Li",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec npx babel-node -r ../../node_modules/dotenv/config src/index.js",
    "build": "npx babel src -d dist --copy-files",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.js'",
    "test": "TESTING=true jest --setupFiles dotenv/config --detectOpenHandles"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "pg": "^7.8.1",
    "supertest": "^3.4.2"
  },
  "devDependencies": {
    "@babel/node": "7.2.2",
    "eslint": "5.15.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "jest": "24.1.0",
    "nodemon": "1.18.10",
    "prettier": "1.16.4"
  }
}
